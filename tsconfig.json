{
    "compilerOptions": {
        "module": "commonjs",
        "incremental": true,
        "tsBuildInfoFile": "./features/bin_js/ts_compiler.increment.txt",
        "noImplicitReturns": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "outDir": "./features/bin_js",
        "sourceMap": true,
        "strict": true,
        "target": "es2020",
        "noImplicitOverride": true,
        "allowJs": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true /* Disallow inconsistently-cased references to the same file. */
    },
    "compileOnSave": true,
    "include": [
        "src/support",
    ],
}